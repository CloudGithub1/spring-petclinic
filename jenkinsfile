pipeline {
    agent any 
    stages {
        stage('gitclone') { 
            steps {
                git branch: 'main', credentialsId: 'private_key', url: 'https://github.com/awscloud1/spring-petclinic.git'
                
            }
        }
      stage('maven pakage build') { 
            steps {
                 sh "./mvnw package"
            }
        }
        stage('Build') { 
            steps {
                sh ' docker build -t awscloud1/spring-petclinic . --build-arg JAR_FILE=target/spring-petclinic-2.4.2.jar'
            }
        }
    }
}
